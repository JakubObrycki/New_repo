pipeline {
  agent any
  environment {
    //DOCKER_CREDENTIALS = credentials('jenkins-token')
    //dockerTag='zomaimage'
    //imageName='web'
    Ec2 = ''
  }

  stages {
    stage ('Integration with EC2'){
      steps {
          sshagent(['jenkins-aws']) {
            sh 'ssh -o StrictHostKeyChecking=no ec2-user@${Ec2}'
            sh 'pwd' 
            sh 'aws --version'
            sh 'ls -lt'
        }
      }
    }
  }
}
